{
  "name": "react-demo-tab",
  "version": "0.0.0-development",
  "description": "A React component to easily create demos of other components",
  "module": "./dist/index.esm.js",
  "main": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "npm run storybook",
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.dev.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "commit": "git-cz",
    "test": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "TS_NODE_PROJECT=tsconfig.dev.json start-storybook -p 9009",
    "storybook-build": "build-storybook",
    "semantic-release": "semantic-release --branches main"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addons": "^6.5.0-beta.0",
    "@storybook/builder-webpack5": "^6.5.0-beta.0",
    "@storybook/manager-webpack5": "^6.5.0-beta.0",
    "@storybook/react": "^6.5.0-beta.0",
    "@storybook/theming": "^6.5.0-beta.0",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-array-reduce": "^1.0.55",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-testing-library": "^5.3.1",
    "husky": "^7.0.4",
    "jest": "^28.0.1",
    "jest-config": "^28.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "tsc-files": "^1.1.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "react-syntax-highlighter": "^15.5.0",
    "react-tabs": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkosir/react-demo-tab.git"
  },
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkosir/react-demo-tab/issues"
  },
  "homepage": "https://mkosir.github.io/react-demo-tab",
  "keywords": [
    "react",
    "react-component",
    "demo-component",
    "demo",
    "demo-tab",
    "demotab",
    "code",
    "tab"
  ]
