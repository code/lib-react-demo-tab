{
  "name": "react-demo-tab",
  "version": "0.0.0-development",
  "description": "React component to easily create demos of other components",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "npm run storybook",
    "prepare": "husky install",
    "build": "shx rm -rf dist && rollup -c",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.dev.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "commit": "git-cz",
    "test": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "TS_NODE_PROJECT=tsconfig.dev.json start-storybook -p 9009",
    "storybook-build": "build-storybook",
    "semantic-release": "semantic-release --branches main"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addons": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/theming": "^6.5.5",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.2",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-array-reduce": "^1.0.59",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-config": "^28.1.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.75.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "semantic-release": "^19.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.3",
    "tsc-files": "^1.1.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@rehooks/local-storage": "^2.4.4",
    "react-syntax-highlighter": "^15.5.0",
    "react-tabs": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkosir/react-demo-tab.git"
  },
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkosir/react-demo-tab/issues"
  },
  "homepage": "https://mkosir.github.io/react-demo-tab",
  "keywords": [
    "react",
    "react-component",
    "demo-component",
    "demo",
    "demo-tab",
    "demotab",
    "code",
    "tab"
  ]
